<div class="text-center">
<h2>Welcome to Image Sharer Application!</h2>
</div>
<div>
  <br/>
  <h2>Saved Images</h2>

 <table>

    <% @image.each do |img| %>
      <tr>
        <td> <img src='<%=img.url%>' width='400' height='400'></td>
        <td>Tags: <%= img.tag_list %></td>
      </tr>
    <% end %>
  </table>

</div>
